    "build": "npm run build:dev && npm run security",
    "build:copy": "copyfiles -f ./src/index.tmpl.html ./src/favicon.ico ./dist",
    "build:dev": "npm run build:messages && npm run build:lint && npm run test && npm run build:dist",
    "build:dev:chrome": "npm run clean && mkdirp src/messages && npm run build:messages && npm run build:lint && npm run test:chrome && npm run build:readme && npm run build:doc && npm run build:dist",
    "build:dist": "npm run build:readme && npm run build:doc && npm run build:copy & webpack --progress -p --env=dist",
    "build:doc": "esdoc -c esdoc.json",
    "build:lint": "stylelint **/*.scss --syntax scss && eslint --ext .js,.jsx ./src",
    "build:prod": "npm prune --production",
    "build:readme": "node-readme",
    "build:messages": "mkdirp src/messages && generate-messages ./src > ./src/messages/en.json",
    "clean": "rimraf README.md src/messages dist/* coverage/* doc/*",
    "release:publish": "git add -A && git push && git push --tags && npm publish",
    "release:major": "npm version major && npm run release:publish && git push --follow-tags",
    "release:minor": "npm version minor && npm run release:publish && git push --follow-tags",
    "release:patch": "npm version patch && npm run release:publish && git push --follow-tags",
    "release:prerelease": "npm version prerelease && npm run release:publish && git push --follow-tags",
    "release:npm": "curl -X GET http://registry.ecd.axway.int:8081/artifactory/local-npm/$npm_package_name/-/$npm_package_name-$npm_package_version.tgz 2>/dev/null > $npm_package_name-$npm_package_version.tgz && npm publish --registry  https://registry.npmjs.org ./$npm_package_name-$npm_package_version.tgz && rm ./$npm_package_name-$npm_package_version.tgz",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "test:watch-no-coverage": "karma start karma-no-isparta.conf.js --autoWatch=true --singleRun=false",
    "test:chrome": "karma start --browsers Chrome",
    "test:chrome:watch": "karma start karma-no-isparta.conf.js --autoWatch=true --singleRun=false --browsers Chrome",
    "test:chrome-coverage:watch": "karma start --autoWatch=true --singleRun=false --browsers Chrome",
    "security": "retire --package --exitwith 0 --outputpath scan-retire.txt && nsp check --output writefile --warn-only"
